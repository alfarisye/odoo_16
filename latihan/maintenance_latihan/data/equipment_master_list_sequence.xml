<?xml version="1.0" encoding="utf-8" ?>
<odoo >
    <!-- Sequence for material request -->
    <record id="equipment_master_list_sequence" model="ir.sequence">
        <field name="name">Equipment Master List Sequence</field>
        <field name="code">equipment.master.list.seq</field>
        <field name="active">TRUE</field>
        <field name="prefix">EQ/%(y)s%(month)s</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>
</odoo>
